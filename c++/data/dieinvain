	ad2xz[1]=-nx;
	ad2xz[2]=-nx+1;
	ad2xz[3]=-1;
	ad2xz[4]=1;
	ad2xz[5]=nx-1;
	ad2xz[6]=nx;
	ad2xz[7]=nx+1;
	
