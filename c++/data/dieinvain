	bdx2z[1]=1;
	bdx2z[2]=-2;
	bdx2z[3]=4;
	bdx2z[4]=-2;
	bdx2z[5]=1;
	bdx2z[6]=-1;
	
	float *Cbdx2z=new float[7];
	Cbdx2z[0]=-nx-1;
	Cbdx2z[1]=-nx;
	Cbdx2z[2]=-1;
	Cbdx2z[3]=0;
	Cbdx2z[4]=1;
	Cbdx2z[5]=nx;
	Cbdx2z[6]=nx+1;
	
