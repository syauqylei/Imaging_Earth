	Cadx2z[1]=-nx;
	Cadx2z[2]=-nx+1;
	Cadx2z[3]=-1;
	Cadx2z[4]=1;
	Cadx2z[5]=nx-1;
	Cadx2z[6]=nx;
	Cadx2z[7]=nx+1;
	
